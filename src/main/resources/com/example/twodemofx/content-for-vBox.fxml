<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.twodemofx.Controllers.AddContentInVBoxController" fx:id="anchorPaneOnVbox" prefHeight="50.0" prefWidth="200.0" style="-fx-border-color: #8C271E; -fx-border-width: 0 0 1px 0;">
    <children>
        <ImageView fx:id="copyEntry" onMouseClicked="#clickOnCopyEntry" fitHeight="37.0" fitWidth="35.0" layoutX="427.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="65.0" AnchorPane.topAnchor="5.0">
            <image>
                <Image url="@../../../icon/copy-icon.png" />
            </image>
        </ImageView>
        <ImageView fx:id="goToWeb" onMouseClicked="#clickOnGoToWeb" fitHeight="37.0" fitWidth="35.0" layoutX="475.0" layoutY="5.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="21.0">
            <image>
                <Image url="@../../../icon/goWeb-icon.png" />
            </image>
        </ImageView>
        <Label fx:id="nameEntry" onMouseClicked="#doubleClickEvent" layoutX="25.0" layoutY="14.0" text="Потраченные часы" AnchorPane.leftAnchor="20.0">
            <font>
                <Font name="Arial Bold" size="20.0" />
            </font>
        </Label>
        <ImageView fx:id="deleteEntry" onMouseClicked="#clickOnDelete" fitHeight="37.0" fitWidth="35.0" layoutX="367.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true" AnchorPane.rightAnchor="110.0" AnchorPane.topAnchor="5.0">
            <image>
                <Image url="@../../../icon/delete-icon.png" />
            </image>
        </ImageView>
    </children>
</AnchorPane>
